{
    "name": "markdown-jira",
    "displayName": "Teste",
    "description": "VSCode Extension for converting Markdown formatting to JIRA or vice-versa easily",
    "version": "1.0.0",
    "publisher": "wilmarques",
    "author": {
        "name": "Wiley Marques"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "homepage": "https://github.com/chintans1/markdown-to-jira/blob/master",
    "repository": {
        "type": "git",
        "url": "https://github.com/chintans1/markdown-to-jira.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.convertMarkdownWithMermaid"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertMarkdownWithMermaid",
                "title": "Convert Markdown to JIRA With Mermaid"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "eslint": "^4.11.0",
        "jira2md": "^2.1.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.37"
    }
}
